<?xml version="1.0"?>
<phpunit
  bootstrap="tests/bootstrap.php"
  backupGlobals="false"
  colors="true"
  convertErrorsToExceptions="true"
  convertNoticesToExceptions="true"
  convertWarningsToExceptions="true">
  <testsuites>
    <testsuite name="default">
      <directory prefix="" suffix="Test.php">./tests/</directory>
      <exclude></exclude>
    </testsuite>
  </testsuites>
</phpunit>
